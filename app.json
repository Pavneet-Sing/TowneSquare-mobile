{
  "expo": {
    "name": "TowneSquare",
    "slug": "TowneSquare",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/images/png/appLogo.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.TowneSquare.TowneSquare"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "fde1a842-16d1-47e7-a064-87974daf80de"
      }
    },
    "packagerOpts": {
      "config": "metro.config.js"
    },
    "plugin":[
      [
        "expo-image-picker",
        {
          "photosPermission":"The app accesses your photos"
        }
      ]
    ]
  }
}
