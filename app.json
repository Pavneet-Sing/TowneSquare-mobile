{
  "expo": {
    "name": "TowneSquare",
    "slug": "townesquare",
    "scheme": "townesquare",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/png/appLogo.png",
    "userInterfaceStyle": "light",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.townesquare.townesquare",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "71a48255-fc2a-4387-84c6-f217978c4e60"
      }
    },
    "packagerOpts": {
      "config": "metro.config.js"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos"
        }
      ]
    ]
  }
}
