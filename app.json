{
  "expo": {
    "name": "TowneSquare",
    "slug": "TowneSquare",
    "scheme": "townesquare",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/png/appLogo.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.townesquare.townesquare"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "b527aae0-9353-4bf2-a05a-352c11cde09b"
      }
    },
    "packagerOpts": {
      "config": "metro.config.js"
    },
    "plugins": [
      [
        "expo-image-picker", 
        {
        "photoPermission": "Let TowneSquare access your photo"
        }
      ]
    ]
  }
}
